.page-title.chart-container
  h1 Charts

.row
  <!-- Line Chart -->
  .col-md-6
    .widget-container
      .heading
        i.fa.fa-bar-chart-o
        | Line Chart
      .widget-content.padded
        .chart-graph.line-chart
          #linechart-2 Loading...
          ul.chart-text-axis
            li 1
            li 2
            li 3
            li 4
            li 5
            li 6
            li 7
            li 8
            li 9
            li 10
            li 11
            li 12
          <!-- end Line Chart -->

  <!-- Line Chart -->
  .col-md-6
    .widget-container
      .heading
        i.fa.fa-bar-chart-o
        | Bar chart
      .widget-content.padded.text-center
        .chart-graph
          #barchart-2 Loading...
  <!-- end Line Chart -->

.row
  <!-- Donut Charts -->
  .col-lg-8
    .widget-container
      .heading
        i.fa.fa-bar-chart-o
        | Donut Charts
      .widget-content.clearfix
        .col-sm-4
          .pie-chart1.pie-chart.pie-number data-percent="87" 87%
        .col-sm-4
          .pie-chart2.pie-chart.pie-number data-percent="26" 26%
        .col-sm-4
          .pie-chart3.pie-chart.pie-number data-percent="54" 54%
  <!-- end Donut Charts -->

  <!-- Pie Chart -->
  .col-lg-4
    .widget-container
      .heading
        i.fa.fa-bar-chart-o
        | Pie Chart
      .widget-content.padded
        .pie-chart
          #pie-chart
          ul.chart-key
            li
              span.green
              | Category 1
            li
              span.orange
              | Category 2
            li
              span.red
              | Category 3
            li
              span.blue
              | Category 4
  <!-- end Pie Chart -->

